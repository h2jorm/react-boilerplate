{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "react boilerplate",
  "main": "scripts/start.js",
  "scripts": {
    "start": "node scripts/start",
    "build": "webpack",
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/scripts/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/react-router",
      "<rootDir>/node_modules/redux",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/core-js",
      "<rootDir>/node_modules/lodash"
    ],
    "moduleFileExtensions": [
      "js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leeching/react-boilerplate.git"
  },
  "keywords": [
    "react",
    "webpack",
    "bable",
    "redux",
    "fetch",
    "jest"
  ],
  "author": "Ching Lee <417300981@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leeching/react-boilerplate/issues"
  },
  "homepage": "https://github.com/leeching/react-boilerplate#readme",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "exports-loader": "^0.6.2",
    "history": "^1.17.0",
    "imports-loader": "^0.6.5",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "lodash": "^3.10.1",
    "moment": "^2.11.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-logger": "^2.3.1",
    "redux-thunk": "^1.0.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.10",
    "whatwg-fetch": "^0.10.1"
  },
  "dependencies": {
    "koa": "^1.1.2",
    "koa-logger": "^1.3.0",
    "koa-mount": "^1.3.0",
    "koa-route": "^2.4.2",
    "koa-send": "^3.1.0",
    "koa-static": "^2.0.0"
  }
}
